<!DOCTYPE node PUBLIC
  "-//freedesktop//DTD D-Bus Object Introspection 1.0//EN"
  "http://standards.freedesktop.org/dbus/1.0/introspect.dtd">

<node>
  <interface name="com.mixtelematics.ContextManager">
    <method name="getContext">
      <arg type="i" name="mcc" direction="in"/>
      <arg type="i" name="mnc" direction="in"/>
      <arg type="i" name="spn" direction="in"/>
      <arg type="i" name="num" direction="out"/>
      <arg type="a(a{sv})" name="contexts" direction="out"/>
    </method>
  </interface>
</node>
